[alias]
  ck = checkout
  cm = commit -a -m
  pullom = pull origin master
  pushom = push origin master
  rbom = rebase origin/master
  mt = !git checkout master && git status
  s = status -s
  st = status
  re = remote
  br = branch
  ft = fetch
  lg = log --oneline --decorate --all --graph
  lgn = log --name-only
